= content_for :title do
  programs

= content_for :sidemenu do
  = render 'programs_page/sidebar'

= content_for :main do
  %table.table.table-bordered.table-striped{:style => 'clear: both'}
    %tr
      %th{:width => '100%'}
        Instrument Type
    - @instruments.each do |instrument|
      %tr
        %td
          %a.x-editable-instrument-name{:href => '#'}
            = instrument.name
  %button.btn.btn-primary{'type' => 'button'}
    New Instrument Type
