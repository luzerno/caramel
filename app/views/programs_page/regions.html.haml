= content_for :title do
  programs

= content_for :sidemenu do
  = render 'programs_page/sidebar'

= content_for :main do
  %table.table.table-bordered.table-striped{:style => 'clear: both'}
    %tr
      %th{:width => '280px'} Region
      %th
    -@regions.each do |region|
      %tr
        %td
          %a.x-editable-region-name{:href => '#'}
            = region.name
        %td
          %a.select2-mul-school{:href => '#'}
  %button.btn.btn-primary{'type' => 'button'}
    New Region

