- content_for :title do
  people

- content_for :sidemenu do
  = render 'people_page/sidebar'

= content_for :main do
  %table.table.table-bordered.table-striped{:style => 'clear: both'}
    %tr
      %th{:width => '13px'}
      %th{:width => '290px'} Name
      %th{:width => '280px'} Email
      %th
    -@students.each do |student|
      %tr
        %td
          %a
            %i.icon-remove
        %td
          %a.x-editable-staff-name{:href => "#", 'data-type' => 'user_name'}
            = student.name
        %td
          %a.x-editable-email{:href => '#'}
            = student.email
        %td
          %button.btn.btn-primary{'type' => 'button', 'data-toggle' => 'button'}
            Show Detail
  %button.btn.btn-primary{'type' => 'button'}
    New Student
