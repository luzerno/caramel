						%a{:href => '/session/signout'} Sign out
					%li.dropdown
						%a.dropdown-toggle{'data-toggle' => 'dropdown', :href => ''}
							SEMESTER
							%span.caret
						%ul.dropdown-menu
							%li
								%a{:href => ''}
									Semester One
							%li
								%a{:href => ''}
